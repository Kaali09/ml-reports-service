<html>

<head>
  <link rel="stylesheet" type="text/css" href="style.css">
</head>

<body>
  
  <%if(data.entityName) {%>
    <div style="border: solid 1px #A9A9A9;margin-top:15px;padding: 10px">
        <h4><%=data.entityName %></h4>
    </div>
    <div style="margin-top:20px">
    <h4>Total Observations : <%= data.totalObservations %></h4>
  </div>
  <% } %>

  <%if(data.totalEntities) {%>
    <div style="width:50%;float:left;margin-top:15px">
      <h4>Total <%= data.entityType %>s : <%= data.totalEntities %></h4>
    </div>
    <div style="width:50%;float:right;margin-top:15px">
      <h4><%= data.entityType %>s Observed : <%= data.entitiesObserved %></h4>
    </div>
  <% } %>


  <%if(data.totalScore) {%>
      <div style="width:50%;float:left;margin-top:15px">
        <h4>Total Score : <%= data.totalScore %></h4>
      </div>
      <div style="width:50%;float:right;margin-top:15px">
        <h4>Score Achieved : <%= data.scoreAchieved %></h4>
      </div>
    <% } %>

  
  <% var orderData= data.orderData; %>
  <% for (var r=0;r < orderData.length; r++){  %>
     <img style="width:80%;margin-top:40px;margin-bottom: 40px;margin-left: 10%" src="<%=orderData[r][0].options.filename%>" />
  <% } %>

</body>

</html>